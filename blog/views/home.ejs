<%- include('partials/header') -%>
<% arrayOfPosts.forEach(post => {%>
<a href="/posts/<%= post._id %>">
  <div class="block">
    <h1>
      <%= post.title %>
    </h1>
    <% if (post.content.length>=140) { %>
    <p> <%=post.content.substring(0,140)+"..."%> </p>
    <% } else { %>
    <p> <%=post.content%> </p>
    <% } %>
  </div>
  <% }) %>

</a>
<%- include('partials/footer'); -%>